{
  "name": "ts-ag",
  "version": "1.0.5",
  "description": "Useful TS stuff",
  "bugs": "https://github.com/ageorgeh/ts-ag/issues",
  "author": "Alexander Hornung",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ageorgeh/ts-ag.git"
  },
  "bin": {
    "clean-dist": "dist/scripts/clean-dist.js",
    "ts-alias": "dist/scripts/ts-alias.js"
  },
  "files": [
    "./src",
    "./dist"
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/browser.d.ts",
        "default": "./dist/browser.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsgo && node dist/scripts/ts-alias.js",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepare": "husky",
    "test": "vitest --run",
    "tsc:watch": "concurrently \" npx tsc -w \" \" npx ts-alias -w \""
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.993.0",
    "@aws-sdk/client-s3": "3.993.0",
    "@aws-sdk/s3-request-presigner": "3.993.0",
    "@types/aws-lambda": "8.10.160",
    "@ungap/structured-clone": "1.3.0",
    "chalk": "5.6.2",
    "chokidar": "5.0.0",
    "cookie": "1.1.1",
    "cycle": "1.0.3",
    "dequal": "^2.0.3",
    "devalue": "^5.6.3",
    "get-tsconfig": "^4.13.6",
    "glob": "^13.0.5",
    "jose": "6.1.3",
    "neverthrow": "8.2.0",
    "oxlint": "^1.48.0",
    "radash": "^12.1.1",
    "rehype-parse": "9.0.1",
    "tsc-alias": "1.8.16",
    "tsconfck": "3.1.6",
    "type-fest": "^5.4.4",
    "unified": "11.0.5",
    "valibot": "1.2.0",
    "vfile": "6.0.3"
  },
  "devDependencies": {
    "@actions/languageserver": "^0.3.44",
    "@eslint/js": "^10.0.1",
    "@types/hast": "3.0.4",
    "@types/node": "^24.10.13",
    "@types/ungap__structured-clone": "1.2.0",
    "@typescript/native-preview": "7.0.0-dev.20260204.1",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "2.6.1",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.33.0",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "typescript-svelte-plugin": "0.3.50",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@types/aws-lambda": "8.10.160"
  }
}
